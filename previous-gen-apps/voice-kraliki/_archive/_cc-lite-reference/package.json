{
  "name": "@ocelot-apps/cc-lite",
  "version": "2.0.0",
  "appEnvironment": "beta",
  "type": "module",
  "private": false,
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "scripts": {
    "dev": "cd frontend && pnpm dev",
    "dev:frontend": "cd frontend && pnpm dev",
    "dev:backend": "PORT=3010 HOST=127.0.0.1 tsx watch backend/index.ts",
    "dev:all": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "build": "pnpm prisma generate && tsc -p backend/tsconfig.json && cd frontend && pnpm build",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:frontend": "cd frontend && pnpm build",
    "build:server:bundle": "tsx scripts/build-backend-bundle.ts",
    "build:analyze": "ANALYZER=true pnpm build",
    "build:production": "NODE_ENV=production pnpm build",
    "preview": "cd frontend && pnpm preview",
    "start:backend": "tsx backend/index.ts",
    "start:backend:dist": "node --loader ./scripts/esm-loader.mjs dist/backend/backend/index.js",
    "start:backend:dist:compat": "node --experimental-specifier-resolution=node dist/backend/backend/index.js",
    "start:backend:dist:bundle": "node dist/backend/backend-bundle.mjs",
    "start:backend:production": "NODE_ENV=production tsx backend/index.ts",
    "start:all": "concurrently \"pnpm preview\" \"pnpm start:backend\"",
    "pm2:setup": "bash scripts/pm2-setup.sh",
    "pm2:deploy": "bash scripts/pm2-deploy.sh",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:start:dev": "pm2 start ecosystem.config.cjs --env development",
    "pm2:stop": "pm2 stop cc-light-server cc-light-campaign-worker",
    "pm2:restart": "pm2 restart cc-light-server cc-light-campaign-worker",
    "pm2:reload": "pm2 reload cc-light-server cc-light-campaign-worker",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs cc-light-server cc-light-campaign-worker",
    "pm2:monit": "pm2 monit",
    "pm2:manage": "bash scripts/pm2-manage.sh",
    "lint": "eslint \"backend/routes/**/*.ts\" backend/index.ts \"backend/utils/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "test": "SKIP_DB_TEST_SETUP=true vitest run tests/smoke/basic-smoke.test.ts --reporter=dot",
    "truth-score": "tsx scripts/truth-score.ts",
    "truth-score:fast": "TRUTH_FAST=true tsx scripts/truth-score.ts",
    "db:cluster:start": "tsx scripts/manage-postgres.ts start",
    "db:cluster:stop": "tsx scripts/manage-postgres.ts stop",
    "db:cluster:status": "tsx scripts/manage-postgres.ts status",
    "db:cluster:restart": "tsx scripts/manage-postgres.ts restart",
    "db:cluster:reset": "tsx scripts/manage-postgres.ts reset",
    "truth-dashboard": "echo 'Truth dashboard script not available in this repo' && exit 1",
    "status": "tsx scripts/project-status.ts",
    "status:dashboard": "tsx scripts/project-status.ts --dashboard",
    "status:watch": "tsx scripts/project-status.ts --watch",
    "status:json": "tsx scripts/project-status.ts --json",
    "test:security": "SKIP_DB_TEST_SETUP=false vitest run --config tests/vitest.security.config.ts",
    "test:security:watch": "SKIP_DB_TEST_SETUP=false vitest --config tests/vitest.security.config.ts --watch",
    "test:security:coverage": "SKIP_DB_TEST_SETUP=false vitest run --config tests/vitest.security.config.ts --coverage",
    "test:unit:security": "vitest run --config tests/vitest.unit.config.ts",
    "test:unit:security:watch": "vitest --config tests/vitest.unit.config.ts --watch",
    "test:unit:security:coverage": "vitest run --config tests/vitest.unit.config.ts --coverage",
    "test:unit": "SKIP_DB_TEST_SETUP=true vitest run tests/unit --config vitest.config.ts",
    "test:integration:trpc": "SKIP_DB_TEST_SETUP=false vitest run tests/integration/trpc-api.integration.spec.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "test:coverage:watch": "vitest --coverage --watch --config vitest.config.ts",
    "test:coverage:report": "vitest run --coverage --config vitest.config.ts && echo 'Coverage report generated at coverage/index.html'",
    "test:coverage:unit": "vitest run tests/unit --coverage --config vitest.config.ts",
    "test:coverage:integration": "vitest run tests/integration --coverage --config vitest.config.ts",
    "test:coverage:components": "vitest run tests/components --coverage --config vitest.config.ts",
    "test:coverage:all": "vitest run --coverage --config vitest.config.ts --reporter=verbose",
    "test:coverage:open": "pnpm test:coverage && echo 'Opening coverage report...' && xdg-open coverage/index.html",
    "test:coverage:baseline": "vitest run --coverage --config tests/vitest.coverage-only.config.ts --reporter=default 2>/dev/null || true",
    "test:watch": "vitest --watch",
    "test:runner": "SKIP_DB_TEST_SETUP=true vitest run tests/smoke/basic-smoke.test.ts --reporter=dot",
    "test:working": "SKIP_DB_TEST_SETUP=true vitest run tests/smoke/basic-smoke.test.ts --reporter=dot",
    "test:components": "vitest tests/components --coverage",
    "test:hooks": "vitest tests/hooks --coverage",
    "test:services": "vitest tests/services --coverage",
    "test:utils": "vitest tests/utils --coverage",
    "test:integration": "vitest tests/integration --coverage",
    "test:integration:websocket": "vitest tests/integration/websocket-integration.test.ts --coverage",
    "test:integration:api": "vitest tests/integration/api-integration.test.ts --coverage",
    "test:integration:auth": "vitest tests/integration/auth-integration.test.ts --coverage",
    "test:integration:realtime": "vitest tests/integration/realtime-integration.test.ts --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:auth": "npx playwright test tests/e2e/auth-workflow.spec.ts",
    "test:e2e:dashboard": "npx playwright test tests/e2e/dashboard-workflow.spec.ts",
    "test:e2e:mobile": "npx playwright test tests/mobile/mobile-responsive.spec.ts",
    "test:accessibility": "npx playwright test tests/accessibility/accessibility-suite.spec.ts",
    "test:visual": "npx playwright test tests/visual/visual-regression.spec.ts",
    "test:performance": "npx playwright test tests/performance/performance-suite.spec.ts",
    "test:voice": "npx playwright test tests/voice/multi-language-voice.spec.ts",
    "test:multi-language": "tsx scripts/test-multi-language.ts",
    "test:multi-language:verbose": "tsx scripts/test-multi-language.ts --verbose --output markdown",
    "test:multi-language:performance": "tsx scripts/test-multi-language.ts --suite performance --output html",
    "test:multi-language:integration": "tsx scripts/test-multi-language.ts --suite integration --parallel --output json",
    "demo:autonomous-run": "tsx scripts/auto-demo.ts",
    "typecheck": "tsc --noEmit",
    "db:setup": "node scripts/setup-database.js",
    "db:setup-dev": "bash scripts/setup-dev-db.sh",
    "db:migrate": "pnpm prisma migrate dev",
    "db:generate": "pnpm prisma generate",
    "db:reset": "pnpm prisma migrate reset",
    "db:studio": "pnpm prisma studio",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed-dev": "tsx scripts/seed-dev-data.ts",
    "dev:services": "docker compose -f docker-compose.dev.yml up -d",
    "dev:services:stop": "docker compose -f docker-compose.dev.yml down",
    "dev:services:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "dev:services:restart": "docker compose -f docker-compose.dev.yml restart",
    "dev:test-setup": "bash scripts/test-dev-setup.sh",
    "perf:audit": "node scripts/performance-audit.js",
    "perf:lighthouse": "node scripts/lighthouse-audit.js",
    "perf:bundle": "webpack-bundle-analyzer dist/client/assets --mode static --report bundle-report.html",
    "perf:optimization": "node scripts/performance-optimization.js",
    "perf:monitor": "node scripts/performance-monitor.js",
    "secrets:generate": "tsx scripts/gen-keys.ts",
    "secrets:rotate": "node scripts/rotate-secrets.js --output-env",
    "env:validate": "tsx scripts/validate-env.ts",
    "env:validate:production": "tsx scripts/validate-env.ts --env=production",
    "env:validate:strict": "tsx scripts/validate-env.ts --strict",
    "startup:validate": "tsx scripts/startup-validation.ts",
    "startup:validate:quick": "tsx scripts/startup-validation.ts --skip-external --timeout=10",
    "truth-score:calculate": "tsx scripts/calculate-truth-score.ts",
    "truth-score:json": "tsx scripts/calculate-truth-score.ts --json",
    "lint:packages": "cd ../../packages/telephony-core && pnpm lint"
  },
